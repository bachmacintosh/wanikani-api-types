{
	"name": "@bachmacintosh/wanikani-api-types",
	"version": "0.7.0",
	"description": "Regularly updated type definitions for the WaniKani API",
	"keywords": [
		"wanikani",
		"wanikani api",
		"types"
	],
	"author": "Collin Bachman <collin@bachman.io>",
	"license": "MIT",
	"homepage": "https://wanikani-api-types.bachman.dev",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./dist/v20170710": {
			"import": "./dist/v20170710.js",
			"require": "./dist/v20170710.js",
			"types": "./dist/v20170710.d.ts"
		}
	},
	"scripts": {
		"build": "rimraf dist && tsc",
		"prebuild:docs": "tsc ./src/internal/docs/MethodsTheme.cts --target es2020 --module commonjs --outDir ./dist/internal/docs",
		"build:docs": "typedoc",
		"lint": "eslint . && prettier --check .",
		"lint:fix": "eslint --fix . && prettier --write .",
		"test": "jest ./tests --verbose"
	},
	"files": [
		"dist/**/*.{js,d.ts}"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/bachmacintosh/wanikani-api-types"
	},
	"devDependencies": {
		"@types/jest": "29.2.2",
		"@typescript-eslint/eslint-plugin": "5.42.0",
		"@typescript-eslint/parser": "5.42.0",
		"eslint": "8.27.0",
		"eslint-config-prettier": "8.5.0",
		"jest": "29.2.2",
		"prettier": "2.7.1",
		"rimraf": "3.0.2",
		"typedoc": "0.23.20",
		"ts-jest": "29.0.3",
		"typescript": "4.8.4"
	},
	"peerDependencies": {
		"typescript": "^4.5.0"
	}
}
